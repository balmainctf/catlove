<% include header.ejs %>

<div class="well jumbotron">
    <h1><%= title %></h1>

    <table>
        <thead>
        <tr>
            <th>id</th>
            <th>uid</th>
            <th>title</th>
            <th>content</th>
            <th>createTime</th>
            <th>操作</th>
        </tr>
        </thead>
        <tbody>

        <% demos.forEach(function( demo ){ %>
        <tr>
            <td><%=demo._id%></td>
            <td><%= demo.uid %></td>
            <td><%= demo.title %></td>
            <td><%= demo.content %></td>
            <td><%=demo.createTime%></td>
            <td><a href="javascript:;" onClick="delData('<%=demo._id%>')">Delete</a> | <a href="/modify/<%=demo._id%>">Update</a></td>
        </tr>
        <% }); %>
        </tbody>
    </table>


</div>

<% include footer.ejs %>
